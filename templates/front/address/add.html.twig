{% extends 'base.html.twig' %}

{% block title %}Add an address{% endblock %}

{% block main %}

<div class="container">

<a href="{{ path('app_front_account')}}" class="btn">Retour</a><br>

{{ form_start(form)}}
    {{ form_row(form.name) }}
    {{ form_row(form.firstname) }}
    {{ form_row(form.lastname) }}
    {{ form_row(form.company) }}
    {{ form_row(form.address) }}
    {{ form_row(form.postal) }}
    {{ form_row(form.city) }}
    {{ form_row(form.country) }}
    {{ form_row(form.phone) }}
{{ form_end(form)}}

</div>

{% endblock %}
